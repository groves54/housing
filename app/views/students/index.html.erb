<p id="notice"><%= notice %></p>

<h1>Students</h1>

<table>
  <thead>
    <tr>
      <th>Studentid</th>
      <th>Studentfname</th>
      <th>Studentlname</th>
      <th>Studentstreet</th>
      <th>Studentcity</th>
      <th>Studentzip</th>
      <th>Studentphone</th>
      <th>Studentemail</th>
      <th>Studentdob</th>
      <th>Studentgender</th>
      <th>Studentyear</th>
      <th>Studentnationality</th>
      <th>Studentneeds</th>
      <th>Studentcomments</th>
      <th>Studentstatus</th>
      <th>Studentmajor</th>
      <th>Studentminor</th>
      <th>Advisorid</th>
      <th>Nextid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @students.each do |student| %>
      <tr>
        <td><%= student.studentId %></td>
        <td><%= student.studentFname %></td>
        <td><%= student.studentLname %></td>
        <td><%= student.studentStreet %></td>
        <td><%= student.studentCity %></td>
        <td><%= student.studentZip %></td>
        <td><%= student.studentPhone %></td>
        <td><%= student.studentEmail %></td>
        <td><%= student.studentDoB %></td>
        <td><%= student.studentGender %></td>
        <td><%= student.studentYear %></td>
        <td><%= student.studentNationality %></td>
        <td><%= student.studentNeeds %></td>
        <td><%= student.studentComments %></td>
        <td><%= student.studentStatus %></td>
        <td><%= student.studentMajor %></td>
        <td><%= student.studentMinor %></td>
        <td><%= student.advisorID %></td>
        <td><%= student.nextID %></td>
        <td><%= link_to 'Show', student %></td>
        <td><%= link_to 'Edit', edit_student_path(student) %></td>
        <td><%= link_to 'Destroy', student, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student', new_student_path %>
